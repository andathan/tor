noinst_LIBRARIES += \
  src/trunnel/hs/libor-trunnel-hs.a

if UNITTESTS_ENABLED
noinst_LIBRARIES += \
  src/trunnel/hs/libor-trunnel-hs-testing.a
endif

AM_CPPFLAGS += -I$(srcdir)/src/ext/trunnel -I$(srcdir)/src/trunnel

HS_TRUNNELSOURCES =              \
	src/ext/trunnel/trunnel.c

HS_TRUNNELHEADERS=               \
	src/ext/trunnel/trunnel.h       \
	src/ext/trunnel/trunnel-impl.h

src_trunnel_hs_libor_trunnel_hs_a_SOURCES = $(HS_TRUNNELSOURCES)
src_trunnel_hs_libor_trunnel_hs_a_CPPFLAGS = -DTRUNNEL_LOCAL_H $(AM_CPPFLAGS)

src_trunnel_hs_libor_trunnel_hs_testing_a_SOURCES = $(HS_TRUNNELSOURCES)
src_trunnel_hs_libor_trunnel_hs_testing_a_CPPFLAGS = -DTRUNNEL_LOCAL_H $(AM_CPPFLAGS) $(TEST_CPPFLAGS)
src_trunnel_hs_libor_trunnel_hs_testing_a_CFLAGS = $(AM_CFLAGS) $(TEST_CFLAGS)

noinst_HEADERS += $(HS_TRUNNELHEADERS)
